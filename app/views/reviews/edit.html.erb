<%= object_errors(@review) %>
<br>
<div class='page-title'>
  <h1>Edit Movie Review</h1>
</div>

<div class="form">
  <%= form_with model: @review, url: movie_review_path(@review.movie, @review), class: "review-form" do |form| %>
    <%= render partial: "movie_review_form", locals: { f: form } %>
  <% end %>
</div>
